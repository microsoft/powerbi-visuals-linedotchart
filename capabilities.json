{
  "dataRoles": [
    {
      "name": "Date",
      "kind": "Grouping",
      "displayName": "Date",
      "displayNameKey": "Visual_Date"
    },
    {
      "name": "Values",
      "kind": "Measure",
      "displayName": "Values",
      "displayNameKey": "Visual_Values"
    },
    {
      "name": "Counter",
      "kind": "Measure",
      "displayName": "Counter",
      "displayNameKey": "Visual_Counter"
    }
  ],
  "dataViewMappings": [
    {
      "conditions": [
        {
          "Date": {
            "min": 0,
            "max": 1
          },
          "Values": {
            "min": 0,
            "max": 1
          },
          "Counter": {
            "min": 0,
            "max": 1
          },
          "Labels": {
            "min": 0,
            "max": 1
          }
        }
      ],
      "categorical": {
        "categories": {
          "select": [
            {
              "for": {
                "in": "Date"
              }
            }
          ],
          "dataReductionAlgorithm": {
            "top": {
              "count": 10000
            }
          }
        },
        "values": {
          "select": [
            {
              "bind": {
                "to": "Values"
              }
            },
             {
              "bind": {
                "to": "Counter"
              }
            }
          ]
        }
      }
    }
  ],
  "supportsHighlight": true,
  "objects": {
    "lineoptions": {
      "displayName": "Line",
      "displayNameKey": "Visual_Line",
      "properties": {
        "fill": {
          "displayName": "Fill",
          "displayNameKey": "Visual_Fill",
          "type": {
            "fill": {
              "solid": {
                "color": true
              }
            }
          }
        },
        "lineThickness": {
          "displayName": "Thickness",
          "displayNameKey": "Visual_Thickness",
          "type": {
            "numeric": true
          }
        }
      }
    },
    "dotoptions": {
      "displayName": "Dot",
      "displayNameKey": "Visual_Dot",
      "properties": {
        "percentile": {
          "displayName": "Opacity",
          "displayNameKey": "Visual_Opacity",
         "type": {
            "numeric": true
          }
        },
        "color": {
          "displayName": "Fill",
          "displayNameKey": "Visual_Fill",
          "type": {
            "fill": {
              "solid": {
                "color": true
              }
            }
          }
        },
        "dotSizeMin": {
          "displayName": "Min size",
          "displayNameKey": "Visual_MinSize",
          "type": {
            "numeric": true
          }
        },
        "dotSizeMax": {
          "displayName": "Max size",
          "displayNameKey": "Visual_MaxSize",
          "type": {
            "numeric": true
          }
        }
      }
    },
    "axisOptions": {
      "displayName": "Axes properties",
      "displayNameKey": "Visual_AxisProperties",
      "properties": {
        "show": {
          "type": {
            "bool": true
          }
        },
        "color": {
          "displayName": "Color",
          "displayNameKey": "Visual_Color",
          "type": {
            "fill": {
              "solid": {
                "color": true
              }
            }
          }
        }
      }
    },
    "counteroptions": {
      "displayName": "Counter",
      "displayNameKey": "Visual_Counter",
      "properties": {
        "counterTitle": {
          "displayName": "Title",
          "displayNameKey": "Visual_Title",
          "type": {
            "text": true
          }
        }
      }
    },
    "misc": {
      "displayName": "Animation",
      "displayNameKey": "Visual_Animation",
      "properties": {
        "isAnimated": {
          "displayName": "Animated",
          "displayNameKey": "Visual_Animated",
          "type": {
            "bool": true
          }
        },
        "isStopped": {
          "displayName": "Stop on load",
          "displayNameKey": "Visual_StopOnLoad",
          "type": {
            "bool": true
          }
        },
        "duration": {
          "displayName": "Time",
          "displayNameKey": "Visual_Time",
          "type": {
            "numeric": true
          }
        }
      }
    }
  }
}
